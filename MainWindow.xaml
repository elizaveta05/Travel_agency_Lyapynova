<Window x:Class="Travel_agency_Lyapynova.MainWindow"
        xmlns:navigation="clr-namespace:System.Windows.Navigation;assembly=PresentationFramework"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Travel_agency_Lyapynova"
        mc:Ignorable="d"
          Title="Авторизация" Height="450" Width="800" >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="53"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Background="#FF8EC0EB" Margin="0,0,0,0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="789*"/>
                <ColumnDefinition Width="11*"/>
            </Grid.ColumnDefinitions>
            <Image Width="45" Height="45" HorizontalAlignment="Left" Margin="5,8,0,8" />
            <Button x:Name="btnBack" HorizontalAlignment="Left" Content="Назад" Click="btnBack_Click" Margin="690,15,0,12" Width="88" Background="#FF298FDD"/>
        </Grid>
        <Frame x:Name="FrmMain" ContentRendered="FrmMain_ContentRendered" NavigationUIVisibility="Hidden" Grid.Row="1"/>
    </Grid>
</Window>
